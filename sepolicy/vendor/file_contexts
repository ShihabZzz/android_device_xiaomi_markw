# Binaries
/system/bin/chargeonlymode                                  u:object_r:charger_exec:s0

# Camera
/data/vendor/qcam(/.*)?                                     u:object_r:vendor_camera_data_file:s0

# Fingerprint
/(mnt/vendor)/persist/data(/.*)?                            u:object_r:persist_fingerprint_file:s0

